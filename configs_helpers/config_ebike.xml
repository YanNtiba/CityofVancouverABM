            <!-- Vancouver-specific MATSim scoring parameters | calibrated 2025-07-15 -->
            <!-- global assumptions used for all mode calculations                                                       -->
            <!--   β_m  (marginalUtilityOfMoney)  = 1.0  util / $                                                   -->
            <!--   β_perf (opportunity cost of time) = 6   util / h ↔ value of time ≈ $6 / h                        -->
            <!--   Monetary units represent explicit out-of-pocket costs only                                       -->
            <!--   Time dis-utility = β_perf + mode-specific discomfort                                             -->
            <!--   Sources: 2022 Metro Vancouver Household Travel Survey; TransLink fare tables;                    -->
            <!--            ICBC fuel-price + AAA operating-cost tables; RMOW wage survey                           -->

            <!-- ================================================================================================= -->
            <!-- PARAMETERS DERIVED VIA ADJUSTED TRANSFER FROM LITERATURE (Dingil et al. 2023 / Velbert scenario) -->
            <!-- ================================================================================================= -->

            <!-- ───────────────────────────────────────────── CAR ───────────────────────────────────────────── -->
            <parameterset type="modeParams">
                <param name="mode" value="car" />
                <!-- Justification: Standard assumption. Disutility comes from opportunity cost of time (VTT) and monetary costs. -->
                <param name="marginalUtilityOfTraveling_util_hr" value="0.0" />
                <!-- Justification: Based on CAA 2017 fuel-only cost of ~$0.11/km, adjusted for higher VTT. Your original value is sound. -->
                <param name="monetaryDistanceRate" value="-0.00020" />
                <!-- Justification: Reflects higher car preference in Vancouver vs. a German city (-1.5 in Velbert). Start with a small positive constant. -->
                <param name="constant" value="1.5" />
            </parameterset>

            <!-- ───────────────────────────────────── PUBLIC TRANSIT ────────────────────────────────────────── -->
            <parameterset type="modeParams">
                <param name="mode" value="pt" />
                <!-- Justification: In-vehicle time is more onerous. Adjusted from Velbert's -1.5 to reflect a higher VTT. -->
                <param name="marginalUtilityOfTraveling_util_hr" value="-2.0" />
                <!-- Justification: Represents an average daily cost for non-pass holders (e.g., 2x 1-zone fares). -->
                <param name="dailyMonetaryConstant" value="-5.80" />
                <!-- Justification: TransLink fares are primarily zone-based, not distance-based. -->
                <param name="monetaryDistanceRate" value="0.0" />
                <!-- Justification: Neutral constant. Let the cost and time parameters drive the choice. Velbert was -1.0. -->
                <param name="constant" value="0.0" />
            </parameterset>

            <!-- ───────────────────────────────────────────── WALK ──────────────────────────────────────────────── -->
            <parameterset type="modeParams">
                <param name="mode" value="walk" />
                <!-- Justification: Represents high physical effort and high opportunity cost of time. A standard value for strenuous travel. -->
                <param name="marginalUtilityOfTraveling_util_hr" value="-12.0" />
                <!-- Justification: Small penalty to discourage unrealistically long walk trips that are better served by other modes. -->
                <param name="marginalUtilityOfDistance_util_m" value="-0.001" />
                <!-- Justification: Strong negative constant to ensure walk is primarily chosen for short, access/egress trips. -->
                <param name="constant" value="-10.0" />
            </parameterset>

            <!-- ───────────────────────────────────── PRIVATE BIKE ──────────────────────────────────────────── -->
            <parameterset type="modeParams">
                <param name="mode" value="bike" />
                <!-- Justification: Physical effort is significant (especially in hilly Vancouver), but less than walking. Velbert's -6.5 is too low for a strong cycling city. -->
                <param name="marginalUtilityOfTraveling_util_hr" value="-8.0" />
                <!-- Justification: Plausible maintenance/upkeep cost. -->
                <param name="monetaryDistanceRate" value="-0.0001" />
                <!-- Justification: Positive constant reflects Vancouver's strong private bike culture for those who own one. -->
                <param name="constant" value="2.0" />
            </parameterset>

            <!-- ────────────────────────────── MOBI BIKE SHARE (for Baseline Run) ───────────────────────────── -->
            <parameterset type="modeParams">
                <param name="mode" value="sharing:mobi_bike" />
                <!-- Justification: More effort than a private bike (heavier, less ergonomic) but same as walking. -->
                <param name="marginalUtilityOfTraveling_util_hr" value="-12.0" />
                <!-- Justification: For baseline only. The PPR model in policy scenarios will use the EventHandler instead. -->
                <param name="dailyMonetaryConstant" value="0.0" />
                <param name="monetaryDistanceRate" value="0.0" />
                <!-- Justification: Negative constant to capture friction of rental process (finding stations, availability). -->
                <param name="constant" value="-1.0" />
            </parameterset>

            <!-- ────────────────────────────── MOBI E-BIKE SHARE (for E-Bike Scenario) ─────────────────────── -->
            <parameterset type="modeParams">
                <param name="mode" value="sharing:mobi_ebike" />
                <!-- Justification: Reduced physical effort of an e-bike makes it much more comfortable than a classic bike. -->
                <param name="marginalUtilityOfTraveling_util_hr" value="-6.0" />
                <!-- Justification: All monetary costs are handled by the EventHandler in the run script. MUST BE ZERO. -->
                <param name="dailyMonetaryConstant" value="0.0" />
                <param name="monetaryDistanceRate" value="0.0" />
                <!-- Justification: Start neutral. Let the mode's superior attributes and higher price determine its attractiveness. -->
                <param name="constant" value="0.0" />
            </parameterset>

            <!-- ─────────────────────────────────────────────────── RIDE (taxi / TNC) ─────────────────────────── -->
            <parameterset type="modeParams">
                <param name="mode"                              value="ride"/>
                <!-- ASC neutral – autoserved but costly -->
                <param name="constant"                          value="0.0"/>
                <!-- base flag-drop + booking fee per day ≈ $2.50 -->
                <param name="dailyMonetaryConstant"             value="-2.50"/>
                <param name="dailyUtilityConstant"              value="-1.0"/>
                <!-- comfort; can work on phone during trip -->
                <param name="marginalUtilityOfTraveling_util_hr" value="-6.0"/>
                <!-- fare 1.88 $ /km (typical UberX) → 0.00188 $ /m -->
                <param name="monetaryDistanceRate"              value="-0.00188"/>
                <param name="marginalUtilityOfDistance_util_m"  value="0.0"/>
            </parameterset>
            <!-- ─────────────────────────────────────────────────────────────
                 OTHER (leave as neutral placeholder)
            ───────────────────────────────────────────────────────────── -->
            <parameterset type="modeParams" >
                <param name="constant" value="0.0" />
                <param name="dailyMonetaryConstant" value="0.0" />
                <param name="dailyUtilityConstant" value="0.0" />
                <param name="marginalUtilityOfDistance_util_m" value="0.0" />
                <param name="marginalUtilityOfTraveling_util_hr" value="-6.0" />
                <param name="mode" value="other" />
                <param name="monetaryDistanceRate" value="0.0" />
            </parameterset>
        </parameterset>
    </module>

    <module name="subtourModeChoice" >
        <!-- Only for backwards compatibility.  Defines if only trips from modes list should change mode, or all trips.  Options: fromAllModesToSpecifiedModes fromSpecifiedModesToSpecifiedModes betweenAllAndFewerConstraints  -->
        <param name="behavior" value="fromSpecifiedModesToSpecifiedModes" />
        <!-- Defines the chain-based modes, seperated by commas -->
        <param name="chainBasedModes" value="car,bike" />
        <!-- Defines whether car availability must be considered or not. A agent has no car only if it has no license, or never access to a car -->
        <param name="considerCarAvailability" value="true" />
        <!-- If greater than 0, subtours will also consider coordinates to be at the same location when smaller than set distance. -->
        <param name="coordDistance" value="400.0" />
        <!-- Defines all the modes available, including chain-based modes, seperated by commas -->
        <param name="modes" value="car,pt,bike,walk,sharing:mobi_ebike" />
        <!-- Defines the probability of changing a single trip for a unchained mode instead of subtour. -->
        <param name="probaForRandomSingleTripMode" value="0.15" />
    </module>

