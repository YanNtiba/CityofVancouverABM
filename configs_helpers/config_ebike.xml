    <!-- ============================================================ -->
    <!--  REPLANNING  (plan strategy)                                  -->
    <!-- ============================================================ -->
    <!-- ============================================================ -->
    <!--  REPLANNING  (plan strategy) - CORRECTED FOR 50 ITERATIONS    -->
    <!-- ============================================================ -->
    <module name="replanning">
        <!-- Let agents keep a memory of their 5 best plans. More memory aids stability. -->
        <param name="maxAgentPlanMemorySize" value="5"/>

        <!-- This global parameter is less clear than per-strategy controls.
             We will control the convergence tail explicitly below. It can be removed or set to 0.8. -->
        <param name="fractionOfIterationsToDisableInnovation" value="0.8"/>

        <!--
            The strategies below define what agents do in each iteration.
            The total weight should sum to 1.0. Agents will randomly select one action based on these weights.
            The 'disableAfterIteration' parameter controls the convergence tail.
            For a 50-iteration run, 80% is iteration 40. Innovation will be turned off after iteration 40.
        -->

        <!-- 1. Agent plan selection (70% of agents) -->
        <!-- This strategy runs for the entire simulation. Agents select from their known plans. -->
        <parameterset type="strategysettings">
            <param name="strategyName" value="ChangeExpBeta"/>
            <param name="weight"       value="0.7"/>
        </parameterset>

        <!-- 2. Innovation: Mode Choice (15% of agents) -->
        <!-- This allows agents to try different modes. It will be disabled after iteration 40. -->
        <parameterset type="strategysettings">
            <param name="strategyName"          value="SubtourModeChoice"/>
            <param name="weight"                value="0.15"/>
            <param name="disableAfterIteration" value="40"/>
        </parameterset>

        <!-- 3. Innovation: Re-routing (15% of agents) -->
        <!-- This allows agents to find new routes based on congestion. It will be disabled after iteration 40. -->
        <parameterset type="strategysettings">
            <param name="strategyName"          value="ReRoute"/>
            <param name="weight"                value="0.15"/>
            <param name="disableAfterIteration" value="40"/>
        </parameterset>
    </module>
