    <module name="routing">
        <!-- (a) keep if you really want to wipe the defaults … -->
        <param name="clearDefaultTeleportedModeParams" value="true"/>
        <!-- (b) declare network-based modes -->
        <param name="networkModes" value="car,bike,sharing:mobi_ebike"/>
